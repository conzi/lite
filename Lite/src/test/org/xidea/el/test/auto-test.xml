<test>
	<unit>
		<model>{"var1":1,"object":{}}
		</model>
		<!-- 变量转换 -->
		<case source="-var1+2*3" />
		<case source="+var1+2*3" />
		<!-- 字符转义 -->
		<case>"\u91D1\u5927\u4E3A"</case>
		<case>"\x21\x32\x42"</case>
	</unit>
	<unit>
		<model>{"var1":1,"object":{}}
		</model>
		<!-- 比较运算 -->
		<case>'0' == 0</case>
		<case>0 == null</case>
		<case>'123' == 123</case>
		<case>'0' != 0</case>
		<case>object == object</case>
		<case>object != object+1</case>
		
		<!-- 三目运算 -->
		<case>1>1?1:2</case>
		<case>1>=1?1:2</case>
		<case>1&lt;1?1:2</case>
		<case>1&lt;=1?1:2</case>

		<case>-1>0?1:2</case>
		<case>-1>=0?1:2</case>
		<case>-1&lt;0?1:2</case>
		<case>-1&lt;=0?1:2</case>

		<case>(1?1:3) + (0?5:7)</case>
		<case>1?1:3 + 0?5:7</case>
		<case>1?0?5:7:3</case>
		<case>1?0?5:0?11:13:3</case>
		<case>3 + 0?5:7?23:1</case>
		<case>1?3 + 0?5:7:23 + 0?511:711</case>
		<case>1?3 + 0?5:7:1</case>
		<case>1?1:3 + 0?5:7</case>
		<case>1?0?5:7:3 </case>
		<case>0?0?5:7:3 </case>
		<case>1?0?5:0?11:13:3</case>
		<case>1?1?0?5:0?11:13:3?1?0?5:0?11:13:3:0?11:13:3</case>
		<!-- 字符串操作 -->
		<case>['abc'+123,'abc'*123,'12'*123,'012'*123,'012'+123].join(',')</case>
		<case>['abc'.indexOf('b'),'abc'.lastIndexOf('2')].join(',')</case>
		<case>['123'.substr(1),'123'.substring(2),'123'.substring(-3)].join(',')</case>
		<!-- 数值操作 -->
		<case>(-123).toFixed(2)</case>
	</unit>
