<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
	<application>litecompiler</application>
	<version>3</version>
	
	<!-- Configure java.util.logging -->
	<system-properties>
		<property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
	</system-properties>
	
	<static-files>
		<include path="/**.js" />
		<include path="/**.css" />
		<include path="/**.gfi" />
		<include path="/**.json" />
		<exclude path="/**.xhtml" />
	</static-files>
	<resource-files>
		<include path="/**.*"/>
	</resource-files>
</appengine-web-app>